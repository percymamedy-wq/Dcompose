{
    "name": "percymamedy/dcompose",
    "description": "Laradock scaffolding cli tool.",
    "keywords": ["laravel", "laradock", "console", "cli"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Percy Mamedy",
            "email": "percymamedy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "~6.0",
        "laravel-zero/framework": "5.7.*",
        "symfony/finder": "^4.2",
        "symfony/yaml": "^4.2",
        "zendframework/zend-text": "^2.7"
    },
    "require-dev": {
        "kint-php/kint": "^3.1",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-posix": "0"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["dcompose"]
}
